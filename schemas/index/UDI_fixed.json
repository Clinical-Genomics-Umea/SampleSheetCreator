{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "UID": {
      "type": "string"
    },
    "IndexKitName": {
      "type": "string"
    },
    "DisplayName": {
      "type": "string"
    },
    "Version": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "Adapters": {
      "type": "object",
      "properties": {
        "AdapterRead1": {
          "type": "string"
        },
        "AdapterRead2": {
          "type": "string"
        }
      },
      "required": [
        "AdapterRead1"
      ]
    },
    "FixedLayout": {
      "type": "string"
    },
    "OverrideCyclesPattern": {
      "type": "string"
    },
    "Type": {
      "type": "string"
    },
    "IndexI7Len": {
      "type": "integer"
    },
    "IndexI5Len": {
      "type": "integer"
    },
    "IndexSets": {
      "type": "object",
      "properties": {
        "IndexDual": {
          "type": "array",
          "items":
            {
              "type": "object",
              "properties": {
                "Pos": {"type": "string"},
                "IndexI7Name": {"type": "string"},
                "IndexI5Name": {"type": "string"},
                "IndexI7": {"type": "string"},
                "IndexI5": {"type": "string"}
              },
              "required": [
                "Pos",
                "IndexI7Name",
                "IndexI5Name",
                "IndexI7",
                "IndexI5"
              ]
            },
            "additionalProperties": false
        }
      },
      "required": ["IndexDual"],
      "additionalProperties": false
    }
  },
  "required": [
    "IndexKitName",
    "DisplayName",
    "Version",
    "Description",
    "Adapters",
    "FixedLayout",
    "OverrideCyclesPattern",
    "Type",
    "IndexI7Len",
    "IndexI5Len",
    "IndexSets"
  ]
}
